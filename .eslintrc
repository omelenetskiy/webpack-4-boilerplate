{
    "env": {
      "browser": true,
      "jest": true,
      "es6": true
    },
    "plugins": ["import", "prettier"],
    "extends": ["plugin:prettier/recommended", "airbnb"],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-eval": "error",
      "import/first": "error",
      "prettier/prettier": "error",
      "no-unused-vars": ["error", { "args": "none" }],
      "react/prop-types": 0,
      "import/no-extraneous-dependencies": 0
    }
  }